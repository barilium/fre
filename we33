-- Orion Library Universal Compatibility Update with Music System
-- TÃ¼m executor'larda Ã§alÄ±ÅŸacak ÅŸekilde optimize edilmiÅŸtir

--[[
Orion Library

MultiSelect Element KullanÄ±mÄ±:
    local MultiSelect = Tab:AddMultiSelect({
        Name = "MultiSelect Ã–rneÄŸi",
        Options = {"SeÃ§enek 1", "SeÃ§enek 2", "SeÃ§enek 3"},
        Default = {"SeÃ§enek 1"}, -- VarsayÄ±lan seÃ§ili deÄŸerler (opsiyonel)
        Flag = "multiSelectFlag", -- Flag deÄŸeri (opsiyonel)
        Callback = function(Value)
            -- Value bir tablo olarak seÃ§ili deÄŸerleri iÃ§erir
            print("SeÃ§ili DeÄŸerler:", table.concat(Value, ", "))
        end    
    })

    -- Fonksiyonlar:
    MultiSelect:Refresh(Options, true) -- SeÃ§enekleri gÃ¼nceller (true parametresi mevcut seÃ§imleri temizler)
    MultiSelect:Set({"SeÃ§enek 1", "SeÃ§enek 3"}) -- SeÃ§ili deÄŸerleri ayarlar

Music System Usage:
    local Window = OrionLib:MakeWindow({
        Name = "My Script Hub",
        Music = true, -- MÃ¼zik sistemini etkinleÅŸtir
        SaveConfig = true,
        ConfigFolder = "MyScript"
    })
]]

local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local Mouse = LocalPlayer:GetMouse()
local HttpService = game:GetService("HttpService")
local SoundService = game:GetService("SoundService")

-- Music System Variables
local MusicSystem = {
    CurrentSound = nil,
    IsPlaying = false,
    IsPaused = false,
    MusicList = {},
    Volume = 0.5
}

local MUSIC_LIST_URL = "https://raw.githubusercontent.com/Kanistay/Storage/refs/heads/main/Mizuk.json"

local function LoadMusicList()
    local success, result = pcall(function()
        return game:HttpGet(MUSIC_LIST_URL)
    end)
    if success and result then
        local ok, decoded = pcall(function()
            return HttpService:JSONDecode(result)
        end)
        if ok and type(decoded) == "table" then
            MusicSystem.MusicList = decoded
        end
    end
end

-- UISizeConstraint gÃ¼venlik fonksiyonu - MaxSize < MinSize hatasÄ± iÃ§in
local function SafeUISizeConstraint(parent, minX, minY, maxX, maxY)
    -- Minimum deÄŸerler her zaman maximum deÄŸerlerden kÃ¼Ã§Ã¼k olmalÄ±
    if maxX < minX then maxX = minX end
    if maxY < minY then maxY = minY end
    
    local constraint = Instance.new("UISizeConstraint")
    constraint.MinSize = Vector2.new(minX, minY)
    constraint.MaxSize = Vector2.new(maxX, maxY)
    constraint.Parent = parent
    return constraint
end

-- ImageButton iÃ§in yardÄ±mcÄ± fonksiyonlar
local function HSVtoRGB(h, s, v)
    local r, g, b
    local i = math.floor(h * 6)
    local f = h * 6 - i
    local p = v * (1 - s)
    local q = v * (1 - f * s)
    local t = v * (1 - (1 - f) * s)
    
    i = i % 6
    
    if i == 0 then r, g, b = v, t, p
    elseif i == 1 then r, g, b = q, v, p
    elseif i == 2 then r, g, b = p, v, t
    elseif i == 3 then r, g, b = p, q, v
    elseif i == 4 then r, g, b = t, p, v
    elseif i == 5 then r, g, b = v, p, q
    end
    
    return math.floor(r * 255), math.floor(g * 255), math.floor(b * 255)
end

-- ImageButton oluÅŸturma ve yapÄ±landÄ±rma fonksiyonlarÄ±
local function createImageButton(playerGui)
    local screenGui = Instance.new("ScreenGui")
    screenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    screenGui.Parent = playerGui
    
    local imageButton = Instance.new("ImageButton")
    imageButton.BorderSizePixel = 0
    imageButton.ScaleType = Enum.ScaleType.Fit
    imageButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    imageButton.Image = "rbxassetid://111073133971859"
    imageButton.Size = UDim2.new(0, 35, 0, 35)
    imageButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
    imageButton.Position = UDim2.new(0.35744, 0, 0.26145, 0)
    imageButton.Parent = screenGui
    
    local uiCorner = Instance.new("UICorner")
    uiCorner.CornerRadius = UDim.new(1, 0)
    uiCorner.Parent = imageButton
    
    local uiStroke = Instance.new("UIStroke")
    uiStroke.Parent = imageButton
    
    return imageButton, uiStroke
end

local function setupDragging(guiObject)
    local dragging = false
    local dragStart = nil
    local startPos = nil
    
    local function onInputBegan(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            dragging = true
            dragStart = input.Position
            startPos = guiObject.Position
            
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragging = false
                end
            end)
        end
    end
    
    local function onInputChanged(input)
        if dragging and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
            local delta = input.Position - dragStart
            local newPos = UDim2.new(
                startPos.X.Scale, startPos.X.Offset + delta.X,
                startPos.Y.Scale, startPos.Y.Offset + delta.Y
            )
            guiObject.Position = newPos
        end
    end
    
    guiObject.InputBegan:Connect(onInputBegan)
    UserInputService.InputChanged:Connect(onInputChanged)
end

local function setupRainbowStroke(uiStroke)
    local hue = 0
    local hueSpeed = 0.04
    local thicknessMin = 1
    local thicknessMax = 3.5
    local thicknessSpeed = 0.3
    local direction = 1
    
    task.spawn(function()
        while uiStroke.Parent do
            hue = (hue + hueSpeed) % 1
            local r, g, b = HSVtoRGB(hue, 1, 1)
            uiStroke.Color = Color3.fromRGB(r, g, b)
            
            uiStroke.Thickness = uiStroke.Thickness + (thicknessSpeed * direction)
            
            if uiStroke.Thickness >= thicknessMax then
                direction = -1
            elseif uiStroke.Thickness <= thicknessMin then
                direction = 1
            end
            
            task.wait(0.03)
        end
    end)
end

-- Music System Functions
local function CreateMusicSystem(MainWindow, WindowConfig)
    if not WindowConfig.Music then return end
    
    -- MÃ¼zik listesini harici JSON'dan yÃ¼kle
    LoadMusicList()
    
    -- Music Panel Container
    local MusicPanel = Instance.new("Frame")
    MusicPanel.Name = "MusicPanel"
    MusicPanel.Size = UDim2.new(0, 240, 0, 344) -- Orion ile aynÄ± yÃ¼kseklik
    MusicPanel.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
    MusicPanel.BackgroundTransparency = 0.05
    MusicPanel.BorderSizePixel = 0
    MusicPanel.Visible = false
    MusicPanel.ZIndex = 1000
    
    -- Store reference
    MusicSystem.MusicPanel = MusicPanel
    
    -- Position music panel to the right of MainWindow
    local function UpdateMusicPanelPosition()
        if MainWindow and MainWindow.Parent then
            local pos = MainWindow.AbsolutePosition
            local size = MainWindow.AbsoluteSize
            MusicPanel.Position = UDim2.new(0, pos.X + size.X + 15, 0, pos.Y)
        end
    end
    
    -- Music Panel Corner
    local MusicCorner = Instance.new("UICorner")
    MusicCorner.CornerRadius = UDim.new(0, 15)
    MusicCorner.Parent = MusicPanel
    
    -- Music Panel Stroke with animated glow
    local MusicStroke = Instance.new("UIStroke")
    MusicStroke.Color = Color3.fromRGB(100, 150, 255)
    MusicStroke.Thickness = 2.5
    MusicStroke.Transparency = 0.2
    MusicStroke.Parent = MusicPanel
    
    -- Animated glow effect for music panel
    spawn(function()
        local glowIntensity = 0.2
        local direction = 1
        while MusicPanel and MusicPanel.Parent do
            glowIntensity = glowIntensity + (0.015 * direction)
            if glowIntensity >= 0.7 then
                direction = -1
            elseif glowIntensity <= 0.2 then
                direction = 1
            end
            if MusicStroke and MusicStroke.Parent then
                MusicStroke.Transparency = 1 - glowIntensity
            end
            wait(0.08)
        end
    end)
    
    -- Music Panel Header
