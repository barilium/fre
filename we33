	AddConnection(MinimizeBtn.MouseButton1Up, function()
		if Minimized then
			-- BÃ¼yÃ¼tme iÅŸlemi
			TweenService:Create(MainWindow, TweenInfo.new(0.5, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {Size = UDim2.new(0, 515, 0, 344)}):Play()
			MinimizeBtn.Ico.Image = "rbxassetid://7072719338"
			wait(.02)
			MainWindow.ClipsDescendants = false
			TabHolder.Visible = true
			WindowTopBarLine.Visible = true
			if ButtonContainer then ButtonContainer.Visible = true end
		else
			MainWindow.ClipsDescendants = true
			WindowTopBarLine.Visible = false
			MinimizeBtn.Ico.Image = "rbxassetid://7072720870"
			WindowName.Text = WindowName.Text
			local ti = TweenService:Create(MainWindow, TweenInfo.new(0.5, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {Size = UDim2.new(0, 515, 0, 40)})
			ti:Play()
			TabHolder.Visible = false
			if ButtonContainer then ButtonContainer.Visible = false end
		end
		-- Minimize/maximize iÅŸlemi sonunda ItemContainer iÃ§indeki UIListLayout'u zorla gÃ¼ncelle
		spawn(function()
			wait(0.04)
			for _, child in ipairs(MainWindow:GetChildren()) do
				if child.Name == "ItemContainer" and child.Visible then
					local layout = child:FindFirstChildWhichIsA("UIListLayout")
					if layout then
						layout.FillDirection = layout.FillDirection -- Yeniden ata, layout'u tetikler
						if child:IsA("ScrollingFrame") then
							child.CanvasSize = UDim2.new(0, layout.AbsoluteContentSize.X, 0, layout.AbsoluteContentSize.Y)
						end
					end
				end
			end
		end)
	end)

